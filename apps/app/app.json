{
  "expo": {
    "name": "Learnify",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-black.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-black.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.learnify.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [["@config-plugins/detox", "sentry-expo"]],
    "extra": {
      "eas": {
        "projectId": "9b9b4ef5-6299-415f-9aff-67ce85aa7136"
      }
    }
  }
}
